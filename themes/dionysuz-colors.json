        "input.background": "#2B303B",
        "input.placeholderForeground": "#746F77",
        "inputOption.activeBorder": "#C668BA",
        "inputValidation.errorBackground": "#ff5757",
        "inputValidation.errorBorder": "#ff5757",
        "inputValidation.infoBackground": "#3A6395",
