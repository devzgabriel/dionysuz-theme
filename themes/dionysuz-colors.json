{
    "$schema": "vscode://schemas/color-theme",
    "name": "Dionysuz Colors",
    "type": "dark",
    "colors": {
        // Contrast Colors
        "contrastBorder": "#0000",
        // Base Colors
        "focusBorder": "#0B0D14",
        "foreground": "#D5CED9",
        "widget.shadow": "#0B0D14",
        "selection.background": "#746F77",
        "errorForeground": "#ff5757",
